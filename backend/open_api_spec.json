{
  "openapi": "3.1.0",
  "info": {
    "title": "freshmeet backend",
    "version": "0.1.3"
  },
  "paths": {
    "/": {
      "get": {
        "operationId": "app_version",
        "responses": {
          "200": {
            "description": "Version of the server",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HomeResponse"
                }
              }
            }
          },
          "500": {
            "description": "Failed to get the vesion of the server",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/recommended": {
      "get": {
        "summary": "Gets recommended meetups",
        "operationId": "recommended_meetups_handler",
        "parameters": [
          {
            "name": "startDate",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "endDate",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Found recommended meetups successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GQLResponse"
                }
              }
            }
          },
          "500": {
            "description": "Failed to fetch meetups",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/search": {
      "post": {
        "summary": "Searches meetups. Event end date will not be set, only even start date will be taken into account.",
        "operationId": "search_handler",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SearchRequestBody"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successfully returned searched meetups",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GQLResponse"
                }
              }
            }
          },
          "500": {
            "description": "Failed to search for meetups",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CovidPrecautions": {
        "type": "object",
        "properties": {
          "venueType": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      },
      "Edge": {
        "type": "object",
        "required": [
          "node",
          "metadata"
        ],
        "properties": {
          "metadata": {
            "$ref": "#/components/schemas/Metadata"
          },
          "node": {
            "$ref": "#/components/schemas/Node"
          }
        }
      },
      "Edge2": {
        "type": "object",
        "required": [
          "node"
        ],
        "properties": {
          "node": {
            "$ref": "#/components/schemas/Node2"
          }
        }
      },
      "Events": {
        "type": "object",
        "required": [
          "edges"
        ],
        "properties": {
          "edges": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Edge2"
            }
          }
        }
      },
      "FeaturedEventPhoto": {
        "type": "object",
        "required": [
          "baseUrl",
          "highResUrl",
          "id"
        ],
        "properties": {
          "baseUrl": {
            "type": "string"
          },
          "highResUrl": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        }
      },
      "FeeSettings": {
        "type": "object",
        "required": [
          "accepts",
          "currency"
        ],
        "properties": {
          "accepts": {
            "type": "string"
          },
          "currency": {
            "type": "string"
          }
        }
      },
      "GQLData": {
        "type": "object",
        "required": [
          "result"
        ],
        "properties": {
          "result": {
            "$ref": "#/components/schemas/MeetupResult"
          }
        }
      },
      "GQLResponse": {
        "type": "object",
        "properties": {
          "data": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/GQLData"
              }
            ]
          },
          "errors": {
            "type": [
              "array",
              "null"
            ],
            "items": {}
          }
        }
      },
      "Group": {
        "type": "object",
        "required": [
          "id",
          "isNewGroup",
          "isPrivate",
          "name",
          "timezone",
          "urlname"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "isNewGroup": {
            "type": "boolean"
          },
          "isPrivate": {
            "type": "boolean"
          },
          "keyGroupPhoto": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/KeyGroupPhoto"
              }
            ]
          },
          "membershipMetadata": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/MembershipMetadata"
              }
            ]
          },
          "name": {
            "type": "string"
          },
          "timezone": {
            "type": "string"
          },
          "urlname": {
            "type": "string"
          }
        }
      },
      "Group2": {
        "type": "object",
        "required": [
          "urlname"
        ],
        "properties": {
          "urlname": {
            "type": "string"
          }
        }
      },
      "HomeResponse": {
        "type": "object",
        "required": [
          "version"
        ],
        "properties": {
          "version": {
            "type": "string"
          }
        }
      },
      "KeyGroupPhoto": {
        "type": "object",
        "required": [
          "baseUrl",
          "highResUrl",
          "id"
        ],
        "properties": {
          "baseUrl": {
            "type": "string"
          },
          "highResUrl": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        }
      },
      "MeetupResult": {
        "type": "object",
        "required": [
          "pageInfo",
          "totalCount",
          "edges"
        ],
        "properties": {
          "edges": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Edge"
            }
          },
          "pageInfo": {
            "$ref": "#/components/schemas/PageInfo"
          },
          "totalCount": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "MembershipMetadata": {
        "type": "object",
        "required": [
          "role"
        ],
        "properties": {
          "role": {
            "type": "string"
          }
        }
      },
      "Metadata": {
        "type": "object",
        "required": [
          "recId",
          "recSource"
        ],
        "properties": {
          "recId": {
            "type": "string"
          },
          "recSource": {
            "type": "string"
          }
        }
      },
      "Node": {
        "type": "object",
        "required": [
          "dateTime",
          "description",
          "eventType",
          "eventUrl",
          "id",
          "isAttending",
          "isOnline",
          "isSaved",
          "covidPrecautions",
          "group",
          "maxTickets",
          "rsvps",
          "title",
          "socialLabels",
          "rsvpState"
        ],
        "properties": {
          "covidPrecautions": {
            "$ref": "#/components/schemas/CovidPrecautions"
          },
          "dateTime": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "eventType": {
            "type": "string"
          },
          "eventUrl": {
            "type": "string"
          },
          "featuredEventPhoto": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/FeaturedEventPhoto"
              }
            ]
          },
          "feeSettings": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/FeeSettings"
              }
            ]
          },
          "googleMapsUrl": {
            "type": [
              "string",
              "null"
            ]
          },
          "group": {
            "$ref": "#/components/schemas/Group"
          },
          "id": {
            "type": "string"
          },
          "isAttending": {
            "type": "boolean"
          },
          "isAttendingStr": {
            "type": [
              "string",
              "null"
            ],
            "description": "A string description of if this event will be attended or not"
          },
          "isOnline": {
            "type": "boolean"
          },
          "isSaved": {
            "type": "boolean"
          },
          "maxTickets": {
            "type": "integer",
            "format": "int64"
          },
          "rsvpState": {
            "type": "string"
          },
          "rsvps": {
            "$ref": "#/components/schemas/Rsvps"
          },
          "series": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/Series"
              }
            ]
          },
          "socialLabels": {
            "type": "array",
            "items": {}
          },
          "title": {
            "type": "string"
          },
          "venue": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/Venue"
              }
            ]
          }
        }
      },
      "Node2": {
        "type": "object",
        "required": [
          "id",
          "dateTime",
          "isAttending",
          "group"
        ],
        "properties": {
          "dateTime": {
            "type": "string"
          },
          "group": {
            "$ref": "#/components/schemas/Group2"
          },
          "id": {
            "type": "string"
          },
          "isAttending": {
            "type": "boolean"
          }
        }
      },
      "PageInfo": {
        "type": "object",
        "required": [
          "hasNextPage"
        ],
        "properties": {
          "endCursor": {
            "type": [
              "string",
              "null"
            ]
          },
          "hasNextPage": {
            "type": "boolean"
          }
        }
      },
      "Rsvps": {
        "type": "object",
        "required": [
          "totalCount"
        ],
        "properties": {
          "totalCount": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "SearchRequestBody": {
        "type": "object",
        "description": "Body for `/search` route",
        "properties": {
          "per_page": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int32",
            "description": "Events to return per page",
            "minimum": 0
          },
          "query": {
            "type": [
              "string",
              "null"
            ],
            "description": "Search query"
          },
          "start_date": {
            "type": [
              "string",
              "null"
            ],
            "description": "Start date of event"
          }
        }
      },
      "Series": {
        "type": "object",
        "required": [
          "events"
        ],
        "properties": {
          "events": {
            "$ref": "#/components/schemas/Events"
          }
        }
      },
      "Venue": {
        "type": "object",
        "required": [
          "id",
          "name",
          "lat",
          "lon",
          "city",
          "state",
          "country"
        ],
        "properties": {
          "city": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "lat": {
            "type": "number",
            "format": "double"
          },
          "lon": {
            "type": "number",
            "format": "double"
          },
          "name": {
            "type": "string"
          },
          "state": {
            "type": "string"
          }
        }
      }
    }
  }
}